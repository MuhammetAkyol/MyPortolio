@model List<MyPortolioUdemy.DAL.Entities.Contact>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Layout/Index.cshtml";

}


<div class="content">
    <div class="container-fluid">
        <h4 class="page-title">İletişim Bilgilerim</h4>
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-body">
                        <table class="table mt-3">
                            <thead>
                                <tr>
                                    <th scope="col">#</th>
                                    <th scope="col">Başlık</th>
                                    <th scope="col">Konu</th>
                                    <th scope="col">Tel 1</th>
                                    <th scope="col">Tel 2</th>
                                    <th scope="col">Mail Adresi</th>
                                    <th scope="col">Mail Adresi 2</th>
                                    <th scope="col">Adres</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>@item.ContactId</td>
                                        <td>@item.Title</td>
                                        <td>@item.Description</td>
                                        <td>@item.Phone1</td>
                                        <td>@item.Phone2</td>
                                        <td>@item.Email1</td>
                                        <td>@item.Email2</td>
                                        <td>@item.Adress</td>

                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


